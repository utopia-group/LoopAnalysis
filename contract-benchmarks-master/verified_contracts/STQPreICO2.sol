/// @title Base contract for Storiqa pre-ICO&#13;
contract STQPreICOBase is SimpleCrowdsaleBase, Ownable, InvestmentAnalytics {&#13;
&#13;
    function STQPreICOBase(address token)&#13;
        SimpleCrowdsaleBase(token)&#13;
    {&#13;
    }&#13;
